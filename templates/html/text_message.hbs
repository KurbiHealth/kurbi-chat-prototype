{{#if self}}

<div style="margin-top:100px;display:block;height:50px;">


<style type="text/css">

	.kurbi-aqetrt-patient-icon {
		height:50px;
		width:auto;
		margin-right:8px;
		margin-top:4px;
		margin-bottom:8px;
		float:right;
	}

	.kurbi-aqetrt-text{
		font-size:1.2em;
		color:white;
		line-height:50px;
		height:50px;
		margin-right:8px;
		margin-top:4px;
		margin-bottom:8px;
		padding-left:10px;
		padding-right:10px;
		min-width:150px;
		background-color:#00aaff;
		text-align: center;
		float:right;
		border-radius:5px;
	}


</style>


 <img class="kurbi-aqetrt-patient-icon" src="http://public.foolhardysoftworks.com:9000/backend/icons/PNG/small-image.png" onload="this.onload=null; this.src=kurbi.getPatientIcon();"/>
<div class="kurbi-aqetrt-text">{{text}}</div>



</div>


{{else}}



<div style="margin-top:100px;">


<style type="text/css">

	.kurbi-qetrt-displayName {
		font-weight:bold;
		font-size:.8em;
		margin-bottom:15px;
		color:#444444;

	}

	.kurbi-qetrt-text{
		font-size:1.8em;
		color:#666666;
	}


</style>

<div class="kurbi-qetrt-displayName">{{displayName}}</div>
<div class="kurbi-qetrt-text">{{text}}</div>

</div>

{{/if}}